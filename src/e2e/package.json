  "scripts": {
    "test": "playwright test",
    "test:e2e": "cucumber-js features/**/*.feature",
    "test:e2e:single": "cucumber-js",
    "report": "echo '\nâœ… E2E Test Report generated at: temp/e2e/cucumber-report.html\n' && if command -v xdg-open > /dev/null; then xdg-open ../../temp/e2e/cucumber-report.html; elif command -v open > /dev/null; then open ../../temp/e2e/cucumber-report.html; else echo 'Please open temp/e2e/cucumber-report.html in your browser'; fi",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:cucumber": "cucumber-js"
